    DodawanieTematuZdarzeniaPopUp.Show();
}

function onDodajTematButtonClick( idZdarzenia, temat) {
    //alert('Dodaj temat clicked! ' + 'idZdarzenia = ' + idZdarzenia + 'temat = ' + temat);
    dodajTematDoZdarzenia( idZdarzenia, temat);
    DodawanieTematuZdarzeniaPopUp.Hide();
    //gridViewPlanyPrzedmiotow.PerformCallback();
    $("#szukajButton").click();
}

function dodajTematDoZdarzenia( idZdarzenia, temat) {
    $.ajax({
        type: "POST",
        url: "/Plany/ZapiszZdarzenie",
        data: { ZdarzenieId: idZdarzenia, temat: temat },
        cache: false
    }).fail(function () {
        alert('Błąd podczas dodawania tematu.');
    }).done(function (data) {
        window.location.reload(true);
    });
}

