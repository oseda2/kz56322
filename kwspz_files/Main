    var dataOd = RezerwacjaOdDateEdit.GetDate();
    if (dataOd.getDate() !== d || dataOd.getMonth() !== m || dataOd.getFullYear() !== y) {
        RezerwacjaOdDateEdit.SetDate(ustawionaData);
    }
    if (RezerwacjaOdDateEdit.GetDate() > ustawionaData) {
        RezerwacjaOdDateEdit.SetDate(ustawionaData);
    }
    if (DostepneSaleCombo.GetEnabled()) {
        DostepneSaleCombo.PerformCallback();
    }
}

function onDostepneSaleComboBeginCallback(s, e) {
    document.getElementById("RezerwujButton").disabled = true;
    e.customArgs["czasOd"] = RezerwacjaOdDateEdit.GetDate().toISOString();
    e.customArgs["czasDo"] = RezerwacjaDoDateEdit.GetDate().toISOString();
}

function onDostepneSaleComboEndCallback(s, e) {
    s.SetSelectedIndex(-1);
    document.getElementById("RezerwujButton").style.visibility = "hidden";
    document.getElementById("RezerwujButton").disabled = true;
}

function onDostepneSaleComboCloseUp(s, e) {
    document.getElementById("RezerwujButton").style.visibility = (s.GetSelectedIndex() > -1) ? "visible" : "hidden";
    document.getElementById("RezerwujButton").disabled = !(s.GetSelectedIndex() > -1);
}

function onDodajRezerwacjeButtonClick() {
    document.getElementById("RezerwujButton").style.visibility = "visible";
    document.getElementById("AktualizujButton").style.visibility = "hidden";
    NazwaRezerwacjiTextBox.SetText('');
    $('#idRezerwacjiEdytuj').val(-1);
    $('#idRezerwacjiUsun').val(-1);
    DostepneSaleCombo.SetEnabled(true);
    RezerwacjaSalPopUp.Show();
    DostepneSaleCombo.PerformCallback();
}

function onEdytujRezerwacjeButtonClick() {
    document.getElementById("RezerwujButton").style.visibility = "hidden";
    document.getElementById("AktualizujButton").style.visibility = "visible";
    MojeRezerwacjeGrid.GetSelectedFieldValues(
        'Id;Od;Do;Sala;Budynek;Uwagi;Nazwa',
        function (zaznaczoneWartosci) {
            $('#idRezerwacjiUsun').val(-1);
            $('#idRezerwacjiEdytuj').val(zaznaczoneWartosci[0][0]);
            RezerwacjaOdDateEdit.SetDate(new Date(zaznaczoneWartosci[0][1].getTime()));
            RezerwacjaDoDateEdit.SetDate(new Date(zaznaczoneWartosci[0][2].getTime()));
            DostepneSaleCombo.SetText(zaznaczoneWartosci[0][3] + '; ' + zaznaczoneWartosci[0][4]);
            UwagiMemo.SetText(zaznaczoneWartosci[0][5]);
            NazwaRezerwacjiTextBox.SetText(zaznaczoneWartosci[0][6]);
            DostepneSaleCombo.SetEnabled(false);
            RezerwacjaSalPopUp.Show();
        }
    );
}

function onUsunRezerwacjeButtonClick( pytanie) {
    MojeRezerwacjeGrid.GetSelectedFieldValues(
        'Id',
        function (wartosci) {
            $('#idRezerwacjiEdytuj').val(-1);
            if (confirm(pytanie)) {
                $('#idRezerwacjiUsun').val(wartosci[0]);
                $('#rezerwacjaFormularz').submit();
            }
        }
    );
}

function onMojeRezerwacjeGridBeginCallback(s, e) {
    e.customArgs["pokazujOd"] = PokazujOdDateEdit.GetDate().toISOString();
    e.customArgs["pokazujDo"] = PokazujDoDateEdit.GetDate().toISOString();
}

function onMojeRezerwacjeGridSelectionChanged(s, e) {
    s.GetSelectedFieldValues(
        'TylkoOdczyt',
        function (wartosci) {
            var tylkoOdczyt = true;
            if (wartosci.length > 0) {
                tylkoOdczyt = wartosci[0];
            }
            document.getElementById("EdytujRezerwacjeButton").disabled = tylkoOdczyt;
            document.getElementById("UsunRezerwacjeButton").disabled = tylkoOdczyt;
        }
    );
}

function toggleKierunki(on) {
    if (on) {
        Kierunki.SetEnabled(true);
        Kierunki.PerformCallback();
    } else {
        toggleSpecjalnosci(false);
        Kierunki.SetValue('');
        Kierunki.SetEnabled(false);
    }
}

function toggleSpecjalnosci(on) {
    if (on) {
        Specjalnosci.SetEnabled(true);
        Specjalnosci.PerformCallback();
    } else {
        Specjalnosci.SetValue('');
        Specjalnosci.SetEnabled(false);
    }
}

function onWydzialyValueChanged(s, _e) {
    var value = s.GetValue();
    toggleKierunki(value !== null && value.length > 0);
}

function onKierunkiValueChanged(s, _e) {
    var value = s.GetValue();
    toggleSpecjalnosci(value !== null && value.length > 0);
}

function onKierunkiBeginCallback(_s, e) {
    var wydzialy = Wydzialy.GetValue();
    e.customArgs["wydzialy"] = wydzialy === null ? "" : wydzialy;
}


function onEdytujZdarzenieButtonClick( idZdarzenia) {
