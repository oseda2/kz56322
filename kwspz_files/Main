function PokazZdjece(src, params, caption, cssClasses) {
    var msg =
        '<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"'
        + ' "http://www.w3.org/TR/html4/loose.dtd">'
        + '<html lang="en">\n'
        + '<head>'
        + '<title>' + caption + '<\/title>'
        + '<link rel="stylesheet" href="/Content/blue/blue.css" type="text\/css" />'
        + '<\/head>'
        + '<body><table><tbody><tr><td><img src="' + src + '" border="0" class="' + cssClasses + '" \/><\/td>'
        + '<tr><td style="text-align:right;"><input type="button" onClick="window.close();" value="' + $('#Zamknij').val() + '" \/><\/td><\/tr>'
        + '<\/tbody><\/table>'
        + '<\/body>'
        + '<\/html>';
    var win = window.open('', caption, params);
    win.document.write(msg);
    win.document.close();
    return false;
}

