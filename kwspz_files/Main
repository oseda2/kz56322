function SzukajPlanyBtnClick(adres) {
    if (TrybStudiow.GetValue() === null)
        var trybStVal = -1;
    else
        trybStVal = parseInt(TrybStudiow.GetValue());

    
    var wydzialVal = Wydzialy.GetValue();
    if (wydzialVal === null) {
        wydzialVal = "";
    }

    if (Nabory.GetValue() === null)
        var naborVal = -1;
    else
        naborVal = parseInt(Nabory.GetValue());

    var kierunkiVal = Kierunki.GetValue();
    if (kierunkiVal === null) {
        kierunkiVal = "";
    }

    var specjalnosciVal = Specjalnosci.GetValue();
    if (specjalnosciVal === null) {
        specjalnosciVal = "";
    }

    if (wydzialVal === "" && kierunkiVal === "" && specjalnosciVal === "")
        return alert("Proszę wybrać dostępne filtry");

    window.location = adres + "?TrybStudiowId=" + trybStVal + "&WydzialId=" + wydzialVal + "&naborId=" + naborVal + "&kierunekId=" + kierunkiVal + "&specjalnoscId=" + specjalnosciVal;
}

function onRezerwacjaOdCloseUp(s, e) {
    var ustawionaData = s.GetDate();
    var y = ustawionaData.getFullYear();
    var m = ustawionaData.getMonth();
    var d = ustawionaData.getDate();
    var dataDo = RezerwacjaDoDateEdit.GetDate();
    if (dataDo.getDate() !== d || dataDo.getMonth() !== m || dataDo.getFullYear() !== y) {
        RezerwacjaDoDateEdit.SetDate(ustawionaData);
    }
    if (RezerwacjaDoDateEdit.GetDate() < ustawionaData) {
        RezerwacjaDoDateEdit.SetDate(ustawionaData);
    }
    if (DostepneSaleCombo.GetEnabled()) {
        DostepneSaleCombo.PerformCallback();
    }
}

