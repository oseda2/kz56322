function supportModeInit(enabled) {
    window.supportMode = enabled;
    $('.sensitive').toggleClass('sensitive-hidden', window.supportMode);
    $(document).bind('keydown', function (e) {
        if (e.ctrlKey && e.shiftKey && e.which === 119) {
            window.supportMode = !window.supportMode;
            $('.sensitive').toggleClass('sensitive-hidden', window.supportMode);
            $.ajax({
                type: 'POST',
                url: '/Shared/SetSupportMode?on=' + window.supportMode,
                data: { on: window.supportMode }
            });
        }
    });
}

