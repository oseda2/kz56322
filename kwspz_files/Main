function onRecenzjaMemoKeyUp(s, e, maxLength, counterId) {
    var counter = $("#" + counterId);
    if (counter !== null) {
        var charsLeft = maxLength - s.GetText().length;
        counter.html(charsLeft.toString());
        counter.css("color", charsLeft < 0 ? "red" : "inherit");
    }
}

